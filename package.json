{
  "name": "clipboard.ninja",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "express": "^4.15.2",
    "log-timestamp": "^0.1.2",
    "material-ui": "^0.18.7",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-ga": "^2.2.0",
    "react-helmet": "^5.1.3",
    "react-router-dom": "^4.1.2",
    "react-tap-event-plugin": "^2.0.1",
    "socket.io": "^2.0.1",
    "socket.io-client": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "^6.25.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.0.1",
    "connect-history-api-fallback": "^1.3.0",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.1",
    "detect-port": "^1.1.3",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.51.1",
    "fs-extra": "^4.0.0",
    "html-webpack-plugin": "^2.29.0",
    "inquirer": "^3.2.1",
    "jest": "^20.0.3",
    "most-awesome-spinner-ever": "^1.0.3",
    "object-assign": "^4.1.1",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "promise": "^8.0.1",
    "react-dev-utils": "^3.0.2",
    "react-error-overlay": "^1.0.9",
    "react-snapshot": "^1.0.4",
    "style-loader": "^0.18.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.5.8",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-manifest-plugin": "^1.2.1",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && react-snapshot",
    "server": "node server/index.js",
    "test": "node scripts/test.js --env=jsdom",
    "test-coverage": "node scripts/test.js --coverage --colors --env=jsdom",
    "lint": "eslint ./src"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
