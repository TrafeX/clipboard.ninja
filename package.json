{
  "name": "clipboard.ninja",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "express": "^4.15.5",
    "log-timestamp": "^0.1.2",
    "material-ui": "^0.20.0",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-ga": "^2.4.1",
    "react-helmet": "^5.1.3",
    "react-router-dom": "^4.1.2",
    "socket.io": "^2.0.1",
    "socket.io-client": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.2",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.25.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.0.1",
    "connect-history-api-fallback": "^1.3.0",
    "cross-spawn": "^6.0.3",
    "css-loader": "^0.28.9",
    "dotenv": "^4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.6.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.6",
    "flow-bin": "^0.73.0",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^2.29.0",
    "inquirer": "^5.0.1",
    "jest": "^23.1.0",
    "most-awesome-spinner-ever": "^1.0.3",
    "object-assign": "^4.1.1",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.0.10",
    "promise": "^8.0.1",
    "react-dev-utils": "^5.0.1",
    "react-error-overlay": "^4.0.0",
    "react-snapshot": "^1.0.4",
    "style-loader": "^0.21.0",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^3.1.11",
    "webpack-manifest-plugin": "^1.2.1",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && react-snapshot",
    "server": "node server/index.js",
    "test": "node scripts/test.js --env=jsdom",
    "test-coverage": "node scripts/test.js --coverage --colors --env=jsdom",
    "lint": "eslint ./src && flow check"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
