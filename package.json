{
  "name": "clipboard.ninja",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.15.2",
    "log-timestamp": "^0.1.2",
    "material-ui": "^0.18.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-tap-event-plugin": "^2.0.1",
    "socket.io": "^1.7.3",
    "socket.io-client": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "^6.23.0",
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.1",
    "detect-port": "1.1.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-react-app": "^1.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.45.0",
    "fs-extra": "^3.0.1",
    "html-webpack-plugin": "^2.28.0",
    "inquirer": "^3.0.6",
    "jest": "^20.0.3",
    "most-awesome-spinner-ever": "^1.0.2",
    "object-assign": "^4.1.1",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-loader": "^2.0.5",
    "promise": "^7.1.1",
    "react-dev-utils": "^1.0.0",
    "react-error-overlay": "^1.0.0",
    "react-snapshot": "^1.0.4",
    "style-loader": "^0.17.0",
    "sw-precache-webpack-plugin": "^0.9.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-manifest-plugin": "^1.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && react-snapshot",
    "server": "node server/index.js",
    "test": "node scripts/test.js --env=jsdom",
    "test-coverage": "node scripts/test.js --coverage --colors --env=jsdom",
    "lint": "eslint ./src"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
